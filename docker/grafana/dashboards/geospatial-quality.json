{
  "dashboard": {
    "id": null,
    "title": "Geospatial Data Quality",
    "description": "Monitoring geospatial data quality and accuracy across cities",
    "tags": ["geospatial", "data-quality", "multi-city"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "1m",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Overall Geospatial Match Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(scraper_city_geospatial_match_rate)",
            "legendFormat": "Overall Match Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.9},
                {"color": "green", "value": 0.95}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Coordinate Validation Success",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(scraper_coordinate_validation_success_total[1h])) / sum(rate(scraper_coordinate_validation_total[1h]))",
            "legendFormat": "Validation Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.95},
                {"color": "green", "value": 0.99}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "OSM Building Matches",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(scraper_osm_building_matches_total[1h]))",
            "legendFormat": "OSM Matches/hour",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Geospatial API Health",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(scraper_geospatial_api_success_total[1h])) / sum(rate(scraper_geospatial_api_requests_total[1h]))",
            "legendFormat": "API Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.95},
                {"color": "green", "value": 0.99}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Geospatial Match Rate by City",
        "type": "timeseries",
        "targets": [
          {
            "expr": "scraper_city_geospatial_match_rate",
            "legendFormat": "{{ city }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["min", "max", "mean", "last"]
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Coordinate Validation Failures by City",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(scraper_coordinate_validation_failures_total[5m])",
            "legendFormat": "{{ city }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["min", "max", "mean", "last"]
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Geospatial API Response Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(scraper_geospatial_api_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(scraper_geospatial_api_duration_seconds_bucket[5m]))",
            "legendFormat": "Median",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["min", "max", "mean", "last"]
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Data Source Quality Scores",
        "type": "bargauge",
        "targets": [
          {
            "expr": "scraper_data_source_quality_score",
            "legendFormat": "{{ data_source }} - {{ city }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "Geospatial Processing Queue",
        "type": "timeseries",
        "targets": [
          {
            "expr": "scraper_geospatial_queue_size",
            "legendFormat": "{{ city }} Queue Size",
            "refId": "A"
          },
          {
            "expr": "rate(scraper_geospatial_processed_total[5m])",
            "legendFormat": "{{ city }} Processing Rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["min", "max", "mean", "last"]
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 10,
        "title": "Geospatial Quality Summary",
        "type": "table",
        "targets": [
          {
            "expr": "scraper_city_geospatial_match_rate",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          },
          {
            "expr": "rate(scraper_coordinate_validation_failures_total[1h])",
            "legendFormat": "",
            "refId": "B",
            "format": "table",
            "instant": true
          },
          {
            "expr": "histogram_quantile(0.95, rate(scraper_geospatial_api_duration_seconds_bucket[1h]))",
            "legendFormat": "",
            "refId": "C",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Value #A"},
              "properties": [
                {"id": "displayName", "value": "Match Rate"},
                {"id": "unit", "value": "percentunit"},
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "red", "value": null},
                      {"color": "yellow", "value": 0.9},
                      {"color": "green", "value": 0.95}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Value #B"},
              "properties": [
                {"id": "displayName", "value": "Validation Failures/h"},
                {"id": "unit", "value": "short"},
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "green", "value": null},
                      {"color": "yellow", "value": 10},
                      {"color": "red", "value": 50}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Value #C"},
              "properties": [
                {"id": "displayName", "value": "API Response Time (95th)"},
                {"id": "unit", "value": "s"},
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "green", "value": null},
                      {"color": "yellow", "value": 2},
                      {"color": "red", "value": 5}
                    ]
                  }
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "job": true,
                "instance": true
              },
              "renameByName": {
                "city": "City"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      }
    ]
  }
}