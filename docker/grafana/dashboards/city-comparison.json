{
  "dashboard": {
    "id": null,
    "title": "City Performance Comparison",
    "description": "Detailed comparison of scraping performance between Helsinki and Espoo",
    "tags": ["scraper", "city-comparison", "performance"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "1m",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Listings Scraped - Helsinki vs Espoo",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(scraper_city_listings_scraped_total{city=\"Helsinki\"}[10m])",
            "legendFormat": "Helsinki",
            "refId": "A"
          },
          {
            "expr": "rate(scraper_city_listings_scraped_total{city=\"Espoo\"}[10m])",
            "legendFormat": "Espoo",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["min", "max", "mean", "last"]
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Success Rate Comparison",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(scraper_city_successful_scrapes_total{city=\"Helsinki\"}[10m]) / rate(scraper_city_total_scrapes_total{city=\"Helsinki\"}[10m])",
            "legendFormat": "Helsinki Success Rate",
            "refId": "A"
          },
          {
            "expr": "rate(scraper_city_successful_scrapes_total{city=\"Espoo\"}[10m]) / rate(scraper_city_total_scrapes_total{city=\"Espoo\"}[10m])",
            "legendFormat": "Espoo Success Rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["min", "max", "mean", "last"]
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 3,
        "title": "Geospatial Match Rate Comparison",
        "type": "timeseries",
        "targets": [
          {
            "expr": "scraper_city_geospatial_match_rate{city=\"Helsinki\"}",
            "legendFormat": "Helsinki Geospatial Match Rate",
            "refId": "A"
          },
          {
            "expr": "scraper_city_geospatial_match_rate{city=\"Espoo\"}",
            "legendFormat": "Espoo Geospatial Match Rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["min", "max", "mean", "last"]
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 4,
        "title": "Error Rate Comparison",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(scraper_city_scraping_failures_total{city=\"Helsinki\"}[10m])",
            "legendFormat": "Helsinki Errors",
            "refId": "A"
          },
          {
            "expr": "rate(scraper_city_scraping_failures_total{city=\"Espoo\"}[10m])",
            "legendFormat": "Espoo Errors",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["min", "max", "mean", "last"]
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      },
      {
        "id": 5,
        "title": "Processing Time Comparison",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(scraper_city_processing_duration_seconds_bucket{city=\"Helsinki\"}[10m]))",
            "legendFormat": "Helsinki 95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(scraper_city_processing_duration_seconds_bucket{city=\"Espoo\"}[10m]))",
            "legendFormat": "Espoo 95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.50, rate(scraper_city_processing_duration_seconds_bucket{city=\"Helsinki\"}[10m]))",
            "legendFormat": "Helsinki median",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.50, rate(scraper_city_processing_duration_seconds_bucket{city=\"Espoo\"}[10m]))",
            "legendFormat": "Espoo median",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["min", "max", "mean", "last"]
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      },
      {
        "id": 6,
        "title": "City Performance Summary",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (city)(increase(scraper_city_listings_scraped_total[24h]))",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          },
          {
            "expr": "avg by (city)(rate(scraper_city_successful_scrapes_total[1h]) / rate(scraper_city_total_scrapes_total[1h]))",
            "legendFormat": "",
            "refId": "B",
            "format": "table",
            "instant": true
          },
          {
            "expr": "avg by (city)(scraper_city_geospatial_match_rate)",
            "legendFormat": "",
            "refId": "C",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Value #A"},
              "properties": [
                {"id": "displayName", "value": "Listings (24h)"},
                {"id": "unit", "value": "short"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Value #B"},
              "properties": [
                {"id": "displayName", "value": "Success Rate"},
                {"id": "unit", "value": "percentunit"},
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "red", "value": null},
                      {"color": "yellow", "value": 0.8},
                      {"color": "green", "value": 0.95}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Value #C"},
              "properties": [
                {"id": "displayName", "value": "Geospatial Match Rate"},
                {"id": "unit", "value": "percentunit"},
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "red", "value": null},
                      {"color": "yellow", "value": 0.9},
                      {"color": "green", "value": 0.95}
                    ]
                  }
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "job": true,
                "instance": true
              },
              "renameByName": {
                "city": "City"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40}
      }
    ]
  }
}